<h1><%= title %></h1>
<p>Welcome to <%= title %></p>
<% if (!hasRunKeeper) { %>
  <p><a href="/auth/runkeeper">Add RunKeeper</a></p>
<% } else { %>
  <p>Got RunKeeper user <%=runkeeper.user.name%> from <%=runkeeper.user.location%>. <a href="/auth/runkeeper/logout">Remove RunKeeper</a>.</p>
<% } %>
<% if (!hasDropbox) { %>
  <p><a href="/auth/dropbox">Add Dropbox</a></p>
<% } else { %>
  <p>Got Dropbox user <%=dropbox.user.display_name%> from <%=dropbox.user.country%>. <a href="/auth/dropbox/logout">Remove Dropbox</a>.</p>
<% } %>
